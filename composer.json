{
    "name": "outboundiq/outboundiq-php",
    "description": "Track and monitor outgoing API calls in PHP applications",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "api",
        "monitoring",
        "tracking",
        "http",
        "observability",
        "metrics"
    ],
    "homepage": "https://outboundiq.dev",
    "authors": [
        {
            "name": "OutboundIQ",
            "email": "hello@outboundiq.dev"
        }
    ],
    "support": {
        "email": "hello@outboundiq.dev",
        "issues": "https://github.com/outboundiq-hq/outboundiq-php/issues",
        "source": "https://github.com/outboundiq-hq/outboundiq-php"
    },
    "autoload": {
        "psr-4": {
            "OutboundIQ\\": "src/"
        },
        "files": [
            "src/bootstrap.php"
        ]
    },
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
